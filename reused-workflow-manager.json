{
    "enabledManagers": [
        "regex"
    ],
    "regexManagers": [
        {
            "fileMatch": [
                "^.github/workflows/.+.yaml$"
            ],
            "matchStrings": [
                "uses: (?<depName>.+?\/.+?)\/.+.yaml@v(?<currentValue>.+)"
            ],
            "datasourceTemplate": "github-tags"
        }
    ]
}
